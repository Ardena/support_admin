<!DOCTYPE html>
<html>
<head>
  <title>SupportAdmin</title>
  <%= stylesheet_link_tag    "support_admin/simple-sidebar", "data_turbolinks-track" => true %>
  <%= stylesheet_link_tag    "support_admin/application", "data_turbolinks-track" => true %>
  <%= javascript_include_tag "support_admin/jquery", "data_turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrapper">
    <div id="sidebar-wrapper">
      <ul class="sidebar-nav">
        <li class="sidebar-brand"><%= link_to 'Support Admin', root_path %></li>
        <% @settings.each do |setting| %>
          <li>
            <%= link_to setting.display_name.to_s, "/support_admin/#{ERB::Util.url_encode(setting.class_name.underscore)}" %>
          </li>
        <% end %>
        <li><%= button_to 'Log Out', logout_path, {class: 'btn-link'} %></li>
      </ul>
  </div>
  <div id="page-content-wrapper">
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">
          <%= yield %>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
